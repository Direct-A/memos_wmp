page {
  background: #f5f5f5;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  font-family: -apple-system, BlinkMacSystemFont, PingFang SC, Noto Sans, Noto Sans CJK SC, Microsoft YaHei UI, Microsoft YaHei, WenQuanYi Micro Hei, sans-serif, Segoe UI, Roboto, Helvetica Neue, Arial, "Apple Color Emoji", "Segoe UI Emoji", Segoe UI Symbol, "Noto Color Emoji";
  overflow-x: hidden;
}


.part_top {
  display: flex;
  align-items: center;
}

.vis_icon {
  display: inline-block;
  width: 25rpx;
  height: 25rpx;
  background-size: cover;
  filter: invert(0.3);
}

.PUBLIC {
  background-image: url(data:image/svg+xml;base64,);
}

.PRIVATE {
  background-image: url(data:image/svg+xml;base64,);
}

.quote {
  border-left: 10rpx solid #00000010;
  color: #00000050;
  padding-left: 20rpx;
  margin: 10px 0;
}

.method {
  display: flex;
  position: absolute;
  right: 30rpx;
}

.method .btn {
  background-size: contain;
  background-position: center;
  background-repeat: no-repeat;
  margin-left: 20rpx;
  width: 50rpx;
  height: 50rpx;
}

.method .top {
  background-image: url(data:image/svg+xml;base64,);
}

.method .preview {
  background-image: url(data:image/svg+xml;base64,);
}

.method .delete {
  background-image: url(data:image/svg+xml;base64,);
}

.method .edit {
  background-image: url(data:image/svg+xml;base64,);
}

.method .link {
  background-image: url(data:image/svg+xml;base64,);
}

.line {
  margin: 20rpx 0;
  opacity: 0.2;
}

.listDiv {
  display: flex;
  align-items: flex-start;
}

.maskHeader {
  position: fixed;
  top: 0;
  width: 100%;
  height: 200rpx;
  z-index: 2;
  background: linear-gradient(180deg, #F5F5F5 0%, rgba(239, 239, 239, 0.772917) 58.12%, rgba(217, 217, 217, 0) 100%);
}

.todo-text {
  display: initial;
  vertical-align: middle;
}

.todo-text-done {
  opacity: 0.3;
  /* text-decoration:line-through; */
}

.todo-block {
  /* display: inline-block;
  vertical-align: middle; */
  padding-right: 20rpx;
  /* margin-top: -10rpx; */
  height: 37rpx;
  width: 45rpx;
  background-size: cover;
  flex-shrink: 0;
}

.todo {
  animation: show 2s forwards alternate-reverse infinite ease-in-out;
  background-image: url(data:image/svg+xml;base64,);
}

.done {
  opacity: 0.3;
  background-image: url(data:image/svg+xml;base64,);
}

.searchIcon {
  border-radius: 50%;
  opacity: 0.6;
  backdrop-filter: blur(100rpx);
  border: 1rpx solid #00000020;
  background-size: cover;
  background-image: url(data:image/svg+xml;base64,);
}

.memoCard .link {
  color: #0077e6;
  /* line-height: 80rpx; */
  font-weight: 900;
  font-family: Noto Sans;
  word-break: break-all;
  display: block;
}

.memoCard .link::before {
  content: '🔗';
  opacity: 0.3;
  font-size: 27rpx;
}

.memoCard .link::after {
  content: attr(data-href);
  opacity: 0.3;
  margin-left: 10rpx;
  font-weight: 400;
  /* font-size: 27rpx; */
}

.content .code {
  margin: 10px 0;
  white-space: pre-wrap;
  background-color: #f5f5f5;
  color: #00000080;
  padding: 30rpx 20rpx;
  border-radius: 10rpx;
  font-family: source-code-pro, Monaco, Consolas, "Courier New", -apple-system, BlinkMacSystemFont, PingFang SC, Noto Sans, Noto Sans CJK SC, Microsoft YaHei UI, Microsoft YaHei, WenQuanYi Micro Hei, sans-serif, Segoe UI, Roboto, Helvetica Neue, Arial, "Apple Color Emoji", "Segoe UI Emoji", Segoe UI Symbol, "Noto Color Emoji";
}

.shortCode {
  margin: 10rpx;
  display: inline;
  white-space: pre-wrap;
  background-color: #f5f5f5;
  color: #9c1111;
  padding: 5rpx 20rpx;
  border-radius: 10rpx;
  font-family: source-code-pro, Monaco, Consolas, "Courier New", -apple-system, BlinkMacSystemFont, PingFang SC, Noto Sans, Noto Sans CJK SC, Microsoft YaHei UI, Microsoft YaHei, WenQuanYi Micro Hei, sans-serif, Segoe UI, Roboto, Helvetica Neue, Arial, "Apple Color Emoji", "Segoe UI Emoji", Segoe UI Symbol, "Noto Color Emoji";
}

.tag-span {
  color: #0077e6;
  /* line-height: 80rpx; */
  font-weight: 500;
  font-family: Noto Sans;
}


.spacer {
  width: 100vw;
  background-image: linear-gradient(180deg, #F5F5F5 0%, rgba(245, 245, 245, 0.96) 35.46%, rgba(245, 245, 245, 0) 100%);
  position: fixed;
  top: 0;
  z-index: 1;
}

.img {
  background-color: #f5f5f5;
  max-height: 200rpx;
  max-width: 200rpx;
  border-radius: 10rpx;
}

.halfDialog {
  display: flex;
  position: sticky;
  box-shadow: 0px 10px 10px #f5f5f5;
  top: 180rpx;
  transition: height 0.5s cubic-bezier(0.71, -0.16, 0.25, 1.18);
  overflow: hidden;
  flex-direction: column;
  z-index: 2;
  background: #ffffff;
  border-radius: 20rpx;
  width: calc(100% - 60rpx);
  margin: 10rpx 30rpx;
  box-shadow: 0 0 50rpx #00000008;
}

.title {
  margin: 30rpx;
  color: #00000080;
}

.close {
  transform: rotate(45deg);
  transition: 500ms;
  position: absolute;
  margin-top: 25rpx;
  right: 60rpx;
  width: 50rpx;
  height: 50rpx;
  background-size: cover;
  background-image: url(data:image/svg+xml;base64,);
}

.open {
  transform: rotate(0deg);
  transition: 500ms;
}

.halfDialog textarea {
  width: calc(90% - 60rpx);
  border-radius: 20rpx;
  padding: 30rpx;
  background: #f5f5f5;
  margin: 0rpx 5%;
  margin-bottom: 20rpx;
  height: 800rpx;
  font-size: 30rpx;
  line-height: 1.5;
}

.memos {
  /* margin-top: 200rpx; */
  z-index: 1;
}

.memoCard {
  /* opacity: 0;
  animation: show 300ms 300ms forwards; */
  transform: translateY(20rpx);
  animation: up forwards 500ms 500ms ease-in-out;
  /* white-space: pre-wrap; */
  margin: 20rpx 30rpx;
  padding: 20rpx 30rpx;
  background: #ffffff;
  font-size: 35rpx;
  /* border: 1rpx solid #e5e7eb; */
  border-radius: 20rpx;
  font-weight: 400;
}

.memoCard:last-child {
  margin-bottom: 40rpx;
}

.memoCard .time {
  letter-spacing: -1rpx;
  color: #00000030;
  font-size: 25rpx;
  margin-right: 20rpx;
  margin-left: 10rpx;
  letter-spacing: 2rpx;
  font-family: 'Smiley Sans Oblique';
}


.archivedbtn {
  /* position: sticky;
  bottom: 80rpx;
  box-shadow: 0 0 20px ; */
  opacity: 0.7;
  width: calc(100% - 110rpx);
  height: 100rpx;
  margin: 30rpx;
  border-radius: 30rpx;
  display: flex;
  align-items: center;
  padding-left: 50rpx;
  background-size: cover;
  background-image: url(data:image/svg+xml;base64,);
}

.none {
  margin: 30rpx;
  font-size: 30rpx;
  color: #00000050;
  text-align: center;
  opacity: 0;
  animation: show 300ms 500ms forwards;
}

.memoCard .status {
  color: #00000030;
  font-size: 25rpx;
  padding: 5rpx 10rpx;
  border-radius: 5rpx;
  font-family: 'Smiley Sans Oblique';
  letter-spacing: 2rpx;
}

.memoCard .content {
  /* font-family: "Noto Serif SC"; */
  white-space: pre-line;
  word-break: break-word;
  max-height: 800rpx;
  margin: 10rpx;
  margin-top: 20rpx;
  font-size: 30rpx;
  overflow: scroll;
  color: #313131;
  font-weight: 400;
  line-height: 1.5;
}

.memoCard .fileList {
  display: flex;
  flex-wrap: wrap;
  gap: 10rpx;
  margin-top: 20rpx;
}

.memoCard .imgList {
  display: flex;
  flex-wrap: wrap;
  /* justify-content: space-between; */
  gap: 10rpx;
  margin-top: 20rpx;
}

.memoCard .videoList {
  display: flex;
  flex-direction: column;
  gap: 10rpx;
  margin-top: 20rpx;
}

.memoCard video {
  border-radius: 10rpx;
  width: 100%;
}

.memoCard .file {
  border: 1px solid #cccccc;
  background-color: #f5f5f5;
  padding: 20rpx;
  border-radius: 10rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}

.file::before {
  content: '';
  display: inline-block;
  width: 30rpx;
  height: 30rpx;
  margin-right: 10rpx;
  background-size: contain;
  background-position: center;
  background-repeat: no-repeat;
  background-image: url(data:image/svg+xml;base64,);
}

.counter-block {
  display: flex;
  align-content: center;
  margin: 5px 0;
}

.orderlist-dot {
  flex-shrink: 0;
  padding-right: 10px;
  font-weight: bold;
  color: #07C160;
  font-style: italic;
}

.dotlist-dot {
  padding-right: 10px;
  color: #07C160;
}


@keyframes show {
  from {
    opacity: 0;
  }

  to {
    opacity: 1;
  }
}

@keyframes cardshow {
  from {
    transform: rotate3d(0, 1, 0, 30deg);
  }

  to {
    transform: rotate3d(1, 0, 0, 0deg);
  }
}

@keyframes up {
  from {
    transform: translateY(20rpx);
  }

  to {
    transform: translateY(0);
  }
}

@keyframes halfDialog {
  from {
    height: 100rpx;
  }

  to {
    height: 600rpx;
  }
}

@import 'app-wxa-auto-dark.wxss'