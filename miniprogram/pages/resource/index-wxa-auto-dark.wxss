@media (prefers-color-scheme: dark) {

    .method {
      filter: brightness(0.8);
    }

    .resource .icon {
      filter: invert(1);
    }

    .fileId {
        border-right: rgb(0, 0, 0);
        color: rgb(0, 105, 12);
    }

    .resource {
      background-color: rgb(31, 31, 31);
    }

    .info {
        border-bottom: rgb(0, 0, 0);
    }

    .info .other {
        color: rgba(255, 255, 255, 0.3137254901960784);
    }

    .headerContainer .uploadBtn {
      background-color: rgb(31, 31, 31);
    }

    .header {
        color: rgb(255, 255, 255);
        background: rgba(24, 24, 24, 0.5019607843137255);
    }
}
